.ext_tabControl
{
	position: relative;
	width: 100%;
	height: 100%;
	cursor:default; 
}

.ext_tabControl > .head
{
	height: 100%;
	width: 22px;
	display: inline-block;
	position: absolute;
}

.ext_tabControl > .body
{
	height: 100%;
	border: 1px solid #9ebcc0;
	display: inline-block;
	position: absolute;
	left: 22px;
	right: 0px;
}
.ext_tabControl  > .head > a
{
	display: inline-block;
	position:relative;
	width: 22px;
	height:127px ;/*128px*/
	word-wrap: break-word;
	background:url(images_new/4.3_5.png) no-repeat;
	cursor:default;
	user-select:none; -moz-user-select:none; -webkit-user-select:none;
	color: #000000;
	font-weight: bold;
	margin-bottom: -19px;/*-10px*/
}

.ext_tabControl  > .head > a.selected:FIRST-CHILD
{
	background:url(images_new/4.3_3.png) no-repeat;
	color: #FFFFFF;
	z-index: 1;
}

.ext_tabControl  > .head > a.selected
{	
	width: 22px;
	height:133px;
	background:url(images_new/4.3_4.png) no-repeat;
	color: #FFFFFF;
	z-index: 1;
}


.ext_tabControl  > .head > a > span
{
	width: 100%;
	position: relative;
	display:inline-block;
	top: 30px;
	left: 4px;
}