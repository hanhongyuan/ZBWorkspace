/**
 ******************************************CHECKEDITOR***********************************
 */
.mx .checkEditor
{
	display:inline-block;
    vertical-align:middle;           
    box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;   
	cursor:default;
	font:12px "宋体";/*新增字体设置*/
}

.mx .checkEditor > #img
{
	position:relative;
	display:inline-block;	
	width:16px;
	height:16px;
	top:3px;
	margin-right:2px;/*修改按钮图标与文字之间的距离margin-right:3px为2px*/
	background:url(images/check_editor_unchecked.png) center center no-repeat;
}

.mx .checkEditor.checked > #img
{
	background:url(images/check_editor_checked.png) center center no-repeat;
}

.mx .checkEditor.disabled > #img
{
	background:url(images/check_editor_unchecked_disabled.png) center center no-repeat;
}

.mx .checkEditor.checked.disabled > #img
{
	background:url(images/check_editor_checked_disabled.png) center center no-repeat;
}

.mx .checkEditor > #text
{
	position:relative;
	display:inline-block;
	height:21px;
	line-height:21px;
}
.mx .checkEditor.disabled > #text
{
    color:#555555;	
}

.mx .checkEditor.error
{
	border:1px solid red;
	box-shadow:0 0 2px rgba(255, 0, 0, 0.8); 
}

/**
 ******************************************CHECKLISTEDITOR***********************************
 */
.mx .checkListEditor
{
	overflow-y:hidden;
	cursor:default;
	padding-left: 3px;
}

.mx .checkListEditor .checkEditor
{
	margin-right: 5px;/*将8px改为5px*/
	font:12px "宋体";
}

.mx .checkListEditor.radio .checkEditor > #img
{
	background:url(images/radio_unchecked.png) center center no-repeat;
}

.mx .checkListEditor.radio .checkEditor.checked > #img
{
	background:url(images/radio_checked.png) center center no-repeat;
}

.mx .checkListEditor.radio .checkEditor.disabled > #img
{
	background:url(images/radio_unchecked_disabled.png) center center no-repeat;
}

.mx .checkListEditor.radio .checkEditor.checked.disabled > #img
{
	background:url(images/radio_checked_disabled.png) center center no-repeat;
}

.mx .checkListEditor.error
{
	border:1px solid red;
	box-shadow:0 0 2px rgba(255, 0, 0, 0.8); 
}

/**
 ******************************************TEXTEDITOR***********************************
 */
.mx .textEditor
{	
    display:inline-block;
    vertical-align:middle;        
    background-color:white;
	box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;	
}

.mx .hint 
{
	color:#cccccc;/*aaaaaa*/
}

.mx .textEditor #inputContainer,.mx .disabled.textEditor #inputContainer
{
	width:100%;
	height:100% 
}

.mx .hintEnabled
{
	color:#aaaaaa;
	background-color:#f0f0f0;
}

.mx .textEditor > input
{
	width:100%;/*100%*/
	border-color: #cccccc;
	font: 12px "宋体";
	height: 20px;
	line-height:16px;
}

.mx .textEditor > textarea
{
	outline:none;
	width:100%;
	height:100%;
	overflow-y:auto;
	border-color: #cccccc;
	font: 12px "宋体";
	line-height: 18px;
}
.mx .textEditor.disabled  textarea
{
	outline:none;
	width:100%;
	height:100%;
	overflow-y:auto;
	color:#9B9898;
	background-color: #F0F0F0;
}

.mx .textEditor #resizableSizeGrip
{
	background:url(images/resizable_size_grip.png) right bottom no-repeat;
}

.mx .textEditor.error input[type=text],.mx .textEditor.error input[type=password],.mx .textEditor.error textarea
{
	border:1px solid red;
	box-shadow:0 0 2px rgba(255, 0, 0, 0.8); 
}

#editorTextReadOnlyTip
{
	font-size:12px;
	z-index:99999;
	overflow : hidden;
	position : absolute;
	white-space :normal;
	word-break :break-all;
	word-wrap :break-word;
	background-color : rgb(229,230,240);
	border:1px solid #C5C5C5;
    padding:12px;
    padding-top:8px;
}








/**
 ******************************************CUSTOMEDITOR***********************************
 */
.mx .customEditor
{
	position:relative;
    display:inline-block;
    vertical-align:middle;        
    box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
}

.mx .customEditor #inputContainer
{
    position:absolute;
    left:0;
    top:0;
    right:20px;
    bottom:0;
	margin-right:3px;             
}

.mx .customEditor #inputContainer input
{       
    position:absolute;
    left:0;
    top:0;
    width:100%;
    height:100%;
    border:1px solid #cccccc;
    vertical-align: middle;
    font-family:"宋体";
    font-size:12px;
    line-height: 16px;
}

.mx .customEditor #customButton
{
    position:absolute;      
    top:0;
    bottom:0;
    right:0;
    width:20px;
    height:21px;
}
.mx .customEditor #customButton input[type=button]
{
	padding:0;
	text-align:center;
	background:url(images_new/custome_button_bg.png) no-repeat;
	border:none;
	width:20px;
	height:21px;
	color:black;
}
.mx .customEditor #customButton input[type=button]:hover
{
	padding:0;
	text-align:center;
	background:url(images_new/custome_button_bg_hover.png) no-repeat;
	border:none;
	width:20px;
	height:21px;
	color:black;
}
.mx .customEditor #customButton input[type=button][disabled]
{
	padding:0;
	text-align:center;
	background:url(images_new/custome_button_bg_disabled.png) no-repeat;
	border:none;
	width:20px;
	height:21px;
	color:black;
}
.mx .customEditor.error
{
	border:1px solid red;
	box-shadow:0 0 2px rgba(255, 0, 0, 0.8); 
	height:23px !important;
}







