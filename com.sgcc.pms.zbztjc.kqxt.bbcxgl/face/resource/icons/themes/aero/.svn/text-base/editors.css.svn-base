}


.mx .labelEditor.error
{
	border:1px solid red;
	box-shadow:0 0 2px rgba(255, 0, 0, 0.8); 
}








/**
 ******************************************LINKEDITOR***********************************
 */
.mx .linkEditor 
{
	position: relative;
	display: inline-block;
	vertical-align: middle;
}

.mx .linkEditor a
{
	color: #477bab;
	cursor: pointer;
}

.mx .linkEditor a:hover 
{
	color: #79ace2;
	text-decoration: underline;
	cursor: hand
}

.mx .linkEditor.disabled a 
{
	text-decoration: underline;
	color: #79ace2;
	cursor: point;
}

.mx .linkEditor a span 
{
	position: relative;
	vertical-align: middle;
	display: inline-block;
	height: 100%;
	top:2px;
}

.mx .linkEditor a #text 
{
	margin-left: 3px;
}

.mx .linkEditor a #img 
{
	width: 16px;
	background: no-repeat;
}

.mx .linkEditor.error
{
	border:1px solid red;
	box-shadow:0 0 2px rgba(255, 0, 0, 0.8); 
}











/**
 ******************************************UPDOWNEDITOR***********************************
 */
.mx .upDownEditor
{
	position:relative;
    display:inline-block;
    vertical-align:middle;
    border:1px solid #CCD0D5;
    padding-right:17px; 
	background-color:white; 
    border-radius:3px; -moz-border-radius:3px; -webkit-border-radius:3px;     
    box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;
}

.mx .upDownEditorHiddenButton
{
	padding-right:0px;
}

.mx .upDownEditor #inputDomain
{
	position:relative;
	width:100%;
    height:100%;      
    border:none;                
}

.mx .upDownEditor #buttonContainer
{
	position:absolute;
	top:0;
	bottom:0;
	right:-2px;
	width:17px;			
}

.mx .upDownEditor #upButton,.mx .upDownEditor #downButton 
{
	display:block;
    width:15px; 
    border-left:1px solid transparent;   
    background-repeat:no-repeat;
}

.mx .upDownEditor #upButton
{
	height:9px; 
	border-bottom:1px solid transparent;	
	background:url(images/up_button.png) center center no-repeat;
}
.mx .upDownEditor #downButton 
{
	height:10px; 
	border-top:1px solid transparent;
	background:url(images/down_button.png) center center no-repeat;
}



.mx .upDownEditor #upButton:hover
{		
    background:url(images/up_button_hover.png) center center no-repeat;
    border-left:1px solid #BEC1C4; 
    border-bottom:1px solid #BEC1C4;
}

.mx .upDownEditor #downButton:hover
{			
    background:url(images/down_button_hover.png) center center no-repeat; 
    border-left:1px solid #BEC1C4;  
}

.mx .upDownEditor #buttonContainer:hover #downButton
{
	border-left:1px solid #BEC1C4;
}
.mx .upDownEditor #buttonContainer:hover #upButton
{
	border-bottom:1px solid #BEC1C4;
	border-left:1px solid #BEC1C4;
}

.mx .upDownEditor #inputDomain:hover + div[id=buttonContainer] #upButton
{
	border-left:1px solid #BEC1C4;
	border-bottom:1px solid #BEC1C4;
}
.mx .upDownEditor #inputDomain:hover + div[id=buttonContainer] #downButton
{
	border-left:1px solid #BEC1C4;
}

.mx .upDownEditor #upButton:active
{
    background:url(images/up_button_pressed.png) center center no-repeat;
}

.mx .upDownEditor #downButton:active
{	
    background:url(images/down_button_pressed.png) center center no-repeat;
}



.mx .upDownEditor.disabled #inputContainer:hover + div[id=buttonContainer] #upButton
{
	border-left:1px solid transparent;
	border-bottom:1px solid transparent;
}
.mx .upDownEditor.disabled #inputContainer:hover + div[id=buttonContainer] #downButton
{
	border-left:1px solid transparent;
}

.mx .upDownEditor.disabled:hover #upButton
{
	border-bottom:1px solid transparent;
	border-left:1px solid transparent; 
}
.mx .upDownEditor.disabled:hover #downButton
{
	border-left:1px solid transparent; 
}


.mx .upDownEditor.disabled #upButton, .mx .upDownEditor.disabled #upButton:hover, .mx .upDownEditor.disabled #upButton:active
{
	border-bottom:1px solid transparent;	
	background:url(images/up_button_disabled.png) center center no-repeat;
}

.mx .upDownEditor.disabled #downButton, .mx .upDownEditor.disabled #downButton:hover, .mx .upDownEditor.disabled #downButton:active
{		
	background:url(images/down_button_disabled.png) center center no-repeat;
}

.mx .upDownEditor.disabled #buttonContainer:hover #downButton
{
	border-left:1px solid transparent;
}
.mx .upDownEditor.disabled #buttonContainer:hover #upButton
{
	border-bottom:1px solid transparent;
	border-left:1px solid transparent;
}


.mx .upDownEditor.disabled #buttonContainer #upButton, .mx .upDownEditor.disabled #buttonContainer #downButton
{
	background-color:#F0F0F0;
}

.mx .upDownEditor.error
{
	border:1px solid red;
	box-shadow:0 0 2px rgba(255, 0, 0, 0.8);-moz-box-shadow:0 0 2px rgba(255, 0, 0, 0.8);-webkit-box-shadow:0 0 2px rgba(255, 0, 0, 0.8);
}


.mx .upDownEditor.disabled #inputContainer
{
	color:#3e3636;    
}
.mx .SliderEditor 
{
	padding-top:2px;
}

