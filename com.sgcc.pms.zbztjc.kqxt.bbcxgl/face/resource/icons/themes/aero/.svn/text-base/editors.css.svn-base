/**
 ****************************************** FileEditor ***********************************
 */

.mx .fileEditor, .mx.fileEditor
{
	height: auto;
}

.mx .fileEditor #inputContainer, .mx.fileEditor #inputContainer
{
	position: relative;
	min-height: 21px;
	margin-right: 23px;
	text-overflow: ellipsis;
  	white-space: nowrap;
  	overflow: hidden;
  	border: 1px solid #CCD0D5;
  	background: #F0F1F0;
}

.mx .fileEditor #inputContainer .linkEditor > a > span, .mx.fileEditor #inputContainer .linkEditor > a > span
{
	color: black; 
	text-decoration: underline;
}

.mx .fileEditor.autoWrap #inputContainer, .mx.fileEditor.autoWrap #inputContainer
{
	position: relative;
	min-height: 21px;
	margin-right: 23px;
	text-overflow: ellipsis;
  	white-space: normal;
  	overflow: normal;
  	border: 1px solid #CCD0D5;
  	background: #F0F1F0;
}

.mx .fileEditor.autoWrap #inputContainer .linkEditor > a > span, .mx.fileEditor.autoWrap #inputContainer .linkEditor > a > span
{
	color: black; 
	text-decoration: underline;
}



/**
 ******************************************COMPLEXCONDITIONEDITOR***********************************
 */
.mx .complexConditionEditor
{
	position:relative;
    overflow:hidden;
    background-color:white;
    border: 1px solid #AAAAAA;
}
.mx.complexConditionEditor
{
	position:relative;
    overflow:hidden;
    background-color:white;
    border: 1px solid #AAAAAA;
    border-radius:2px; -moz-border-radius:2px; -webkit-border-radius:2px;
    box-shadow:0 2px 3px rgba(0,0,0,0.1);-moz-box-shadow:0 2px 3px rgba(0,0,0,0.1);-webkit-box-shadow:0 2px 3px rgba(0,0,0,0.1);  
}

.mx .complexConditionEditor table,.mx.complexConditionEditor table
{
	width:100%;
    height:100%;
    border-collapse: collapse;
}

.mx .complexConditionEditor table  tr,.mx.complexConditionEditor table  tr
{
    height:22px;
}

.mx .complexConditionEditor > table > tbody > tr > td,.mx.complexConditionEditor > table > tbody > tr > td
{
	padding-left: 2px;
}

.mx .complexConditionEditor table  tr > td > div.delBtn,.mx.complexConditionEditor table  tr > td > div.delBtn
{
    width:16px;
    height:16px;
    background:url(images/datagrid_dragcol_rejust.png) ;     
}







